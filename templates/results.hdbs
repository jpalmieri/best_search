{{#each results}}
  {{#if this}}
    <tr>
      <td>
        {{#if ../../isDcSearch}}
          {{this.name}}
        {{/if}}
        {{#if ../../isArticleSearch}}
          <a href='{{this.html_url}}'
          target='_blank'>
            {{this.title}}
          </a>
        {{/if}}
        {{#if ../../isOtherSearch}}
          {{this.title}}
        {{/if}}
      </td>
      <td>
        <!-- clone and edit links -->
        {{#if ../../isDcSearch}}
          <a href="/dynamic_content/items/{{this.id}}"
          target="_blank" class='edit-this'>
            edit
          </a>
        {{/if}}
        {{#if ../../isArticleSearch}}
          <!-- don't renter clone or edit links for article results-->
        {{/if}}
        {{#if ../../isMacroSearch}}
          <a href="admin/macros/new?macro_id={{this.id}}">
            clone
          </a>
          <span class="delim">|</span>
          <a href="admin/macros/{{this.id}}" class='edit-this'>
            edit
          </a>
        {{/if}}
        {{#if ../../isOtherSearch}}
          <a href="/rules/new?copy_id={{this.id}}&filter={{../../type}}" target="_blank">
            clone
          </a>
          <span class="delim">|</span>
          <a href="/rules/{{this.id}}/edit" target="_blank" class='edit-this'>
            edit
          </a>
        {{/if}}
      </td>
      <td>{{this.created_at}}</td>
      <td>{{this.updated_at}}</td>
      {{#if ../../isDcSearch}}
        {{#if this.active}}
          <td>Active variants</td>
        {{else}}
          <td>Only default variant</td>
        {{/if}}
      {{/if}}
      {{#if ../../isArticleSearch}}
        {{#if this.draft}}
          <td>Draft</td>
        {{else}}
          <td>Active</td>
        {{/if}}
      {{/if}}
      {{#if ../../isMacroSearch}}
        {{#if this.active}}
          <td>Active</td>
        {{else}}
          <td>Inactive</td>
        {{/if}}
      {{/if}}
      {{#if ../../isOtherSearch}}
        {{#if this.active}}
          <td>Active</td>
        {{else}}
          <td>Inactive</td>
        {{/if}}
      {{/if}}
    </tr>
  {{/if}}
{{/each}}
